{
  "name": "vscode-ros2",
  "displayName": "ROS2",
  "description": "Robot Operation System 2 (ROS2)",
  "version": "0.0.1",
  "publisher": "nonanonno",
  "repository": {
    "type": "git",
    "url": "https://github.com/nonanonno/vscode-ros2"
  },
  "icon": "assets/icon.png",
  "license": "MIT",
  "engines": {
    "vscode": "^1.55.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "ros2"
  ],
  "contributes": {
    "languages": [
      {
        "id": "ros2.interface",
        "aliases": [
          "ROS Interface",
          "interface"
        ],
        "extensions": [
          ".action",
          ".msg",
          ".srv"
        ],
        "configuration": "./lang-config/ros2.interface.json"
      }
    ],
    "grammars": [
      {
        "language": "ros2.interface",
        "scopeName": "source.ros2.interface",
        "path": "./lang-config/syntaxes/ros2.interface.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.12.0",
    "@types/vscode": "^1.56.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^3.9.9",
    "tyscan": "^0.3.2",
    "vscode-test": "^1.5.2"
  }
}
